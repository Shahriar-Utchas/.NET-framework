
@{
    ViewBag.Title = "ChannelDelete";
}

<h1>Are you Sure you want to delete "@Model.ChannelName" Channel?</h1>
<h3></h3>
<form method="post">
    <input type="hidden" name="ChannelId" value="@Model.ChannelId" />
    <input type="submit" value="Yes" name="dcsn" class="btn btn-danger" />
    <input type="submit" value="No" name="dcsn" class="btn btn-warning" />
</form>